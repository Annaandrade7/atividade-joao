<Window x:Class="Atividade.CadrastoAluno"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Atividade"
        mc:Ignorable="d"
        Title="CadrastoAluno" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox Name="txtNome" Text="Nome" Foreground="purple" Height="35" />
            <TextBox Name="txtCpf" Text="Cpf" Foreground="purple" Height="35"/>
            <TextBox Name="txtdatanas" Text="Data Nascimento" Foreground="purple" Height="35"/>
            <TextBox Name="txtTelefone" Text="Telefone" Foreground="purple" Height="35"/>
            <TextBox Name="txtemail" Text="Email" Foreground="purple" Height="35"/>
            <Button Content="Salvar" Height="43" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
